 CONFIG_CFB_CONSOLE=y
 CONFIG_VIDEO_SW_CURSOR=y
 CONFIG_VIDEO_LOGO=y
+CONFIG_VIDEO_BMP_LOGO
 CONFIG_DM_VIDEO=y
 # CONFIG_VIDEO_BPP8 is not set
 # CONFIG_VIDEO_BPP16 is not set
